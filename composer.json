    "keywords": ["payment", "mollie"],
    "homepage": "https://github.com/ruudk/PaymentMollieBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@mphuis.com"
        }
    ],
    "require": {
        "php": "^5.3.2|^7.0",
        "symfony/framework-bundle": "~2.0",
        "jms/payment-core-bundle": "~1.0",
        "psr/log": "~1.0",
        "omnipay/mollie": "~3.0"
    },
    "autoload": {
        "psr-4": { "Ruudk\\Payment\\MollieBundle\\": "src" }
    }
}
